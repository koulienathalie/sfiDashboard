{
  "name": "fortigate-monitor",
  "version": "1.0.0",
  "description": "Tableau de bord de monitoring en temps r√©el pour Fortigate via Elasticsearch",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --names \"BACK,FRONT\" --prefix-colors \"blue,green\" \"npm run start:back\" \"npm run start:front\"",
    "start:back": "cd back && node server.js",
    "start:front": "cd frontend && npm run dev",
    "test:connection": "cd back && node test-connection.js",
    "test:data": "cd back && node test-data.js",
    "install:all": "npm install && cd back && npm install && cd ../frontend && npm install && cd ..",
    "build:front": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "clean": "rm -rf node_modules back/node_modules frontend/node_modules",
    "lint:back": "cd back && npm run lint",
    "logs": "tail -f back/logs/*.log"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "elasticsearch",
    "fortigate",
    "monitoring",
    "firewall",
    "websocket",
    "real-time",
    "dashboard"
  ],
  "author": "",
  "license": "ISC"
}